<% layout( '../../layout' ) -%>

    <div id="wrapper">
            <nav class="navbar navbar-default navbar-fixed-top" role="navigation" style="margin-bottom: 0">

            <%- partial('../../header') %> 
            <!-- /.navbar-header -->
            <%- partial('../../navbar-top-links') %>
            <!-- /.navbar-top-links -->
            <%- partial('../../navbar-static-side') %>
            <!-- /.navbar-static-side -->
        </nav>

        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header"><%- title %></h1>
                </div>
                <ol class="breadcrumb">
                    <li><a href="/repo"><i class="icon-dashboard"></i></a></li>
                    <li class="active"><i class="icon-file-alt"></i></li>
                    <li class="active"><i class="icon-file-alt"></i></li>
                </ol>
            </div>
            <div class="row">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <i class="fa fa-list fa-fw"></i> WT
                    </div>
                    <!-- /.panel-heading -->
                    <div class="panel-body">
                        <div class="col-lg-3 input-group custom-search-form">
                            <input type="text" class="form-control" placeholder="Search..." name="content">
                            <span class="input-group-btn">
                                <button class="btn btn-default" type="button">
                                <i class="fa fa-search"></i>
                                </button>
                            </span>
                        </div>
                        <hr></hr>
                        <div class="col-lg-3 repos-wrapper">
                            <h4>  </h4>
                            <hr></hr>
                            <ul class="nav nav-pills nav-stacked">
                                <li class="active">
                                    <a href="#"> Repo </a></li>
                                <li>
                                    <a href="#"> People </a></li>
                                <li>
                                    <a href="#"> Org </a></li>
                                <li>
                                    <a href="#"> Description </a></li>
                            </ul>
                        </div>
                        <div class="col-lg-9 repos-wrapper">
                            <h4> Result </h4>
                            <hr></hr>
                            <div class="github-widget" data-repo="JoelSutherland/GitHub-jQuery-Repo-Widget"></div>
                        </div>
                    </div>
                    <!-- /.panel-body -->
                </div>
            </div>
        </div>
        <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->
    <script>
        $(function() {
            $('#timeline-list').html("");
        });
    </script>

    <!-- Github repo Script -->
    <script src="/javascripts/jquery.githubRepoWidget.js"></script>

    <!-- Core Scripts - Include with every page -->
    <script src="/javascripts/jquery-1.10.2.js"></script>
    <script src="/javascripts/bootstrap.min.js"></script>
    <script src="/javascripts/plugins/metisMenu/jquery.metisMenu.js"></script>

    <!-- Page-Level Plugin Scripts - Dashboard -->
    <script src="/javascripts/plugins/morris/raphael-2.1.0.min.js"></script>
    <script src="/javascripts/plugins/morris/morris.js"></script>

    <!-- SB Admin Scripts - Include with every page -->
    <script src="/javascripts/sb-admin.js"></script>

    <!-- Page-Level Demo Scripts - Dashboard - Use for reference -->
    <script src="/javascripts/demo/dashboard-demo.js"></script>