<% layout( '../layout' ) -%>

    <div id="wrapper">
            <nav class="navbar navbar-default navbar-fixed-top" role="navigation" style="margin-bottom: 0">

            <%- partial('../header') %> 
            <!-- /.navbar-header -->
            <%- partial('../navbar-top-links') %>
            <!-- /.navbar-top-links -->
            <%- partial('../navbar-static-side') %>
            <!-- /.navbar-static-side -->
        </nav>

        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header"><%= ownerName %> / <%= repoName %></h1>
                </div>
                <!-- /.col-lg-12 -->            
                <ol class="breadcrumb">
                    <li><a href="/repo"><i class="icon-dashboard"></i> Dashboard</a></li>
                    <li class="active"><i class="icon-file-alt"></i> <%= ownerName %></li>
                    <li class="active"><i class="icon-file-alt"></i> <%= repoName %></li>
                </ol>
            </div> 

            <%- partial('state') %> 

            <!-- /.row -->
            <div class="row">     

                <div class="col-lg-4">
                    <!-- /.panel -->
                    <%- partial('repo-list') %>
                    <!-- /.panel -->

                    <!-- /.panel .chat-panel -->
                </div>      

                <div class="col-lg-8">
                    <!-- /.panel -->
                    <%- partial('../component/chart/area-chart') %>
                    <!-- /.panel -->

                    <!-- /.panel .chat-panel -->
                </div>

            </div>
            <!-- /.row -->

            <!-- /.row -->
            <div class="row">               
                <div class="col-lg-12">
                    <!-- /.panel -->
                    <%- partial('build_table') %>
                    <!-- /.panel -->
                </div>
                <!-- /.col-lg-8 -->
            </div>
            <!-- /.row -->
        </div>
        <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->

    <!-- Core Scripts - Include with every page -->
    <script src="/javascripts/jquery-1.10.2.js"></script>
